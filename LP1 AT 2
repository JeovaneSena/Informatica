Quest達o 1

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

main () {
int i;
	for (i = 1; i<= 100; i++)
   printf ("%d\n", i);
   system ("pause");
   }

Quest達o 2

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main (void) {
int a = 1;

	while (a <= 1000)
   {
   if (a%2 == 0)
   printf ("%d \n", a);
   a++;
   }
   system ("pause");
   }

Quest達o 3

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main (void) {

int a, b, i, t1, t2, v1, v2;

	printf ("Digite o primeiro numero: ");
   scanf ("%d", &a);
   printf ("Digite o segundo numero: ");
   scanf ("%d", &b);
   if (a < b)
   {
   t1 = a;
   }
   else
   {
   t1 = b;
   }

   if (b > a)
   {
   t2 = b;
   }
   else
   {
   t2 = a;
   }
   v1 = t1;
   v2 = t2;

   if (v1 % 1 == 0)
   v1 < v2 + 1;
   else
   v1 > v2 +1;

   printf ("Numeros do intervalo:\n");

   for (i = v1; i <= v2; i = i+1)
   {
	printf("%d\n",i);
	}


   system ("pause");
   }
   
